{
  "name": "server-mockr",
  "version": "1.0.5",
  "description": "Package to mock HTTP APIs for fast and reliable testing.",
  "main": "dist/index.js",
  "files": [
    "dist",
    "!__tests__",
    "!*.tsbuildinfo"
  ],
  "scripts": {
    "build": "tsc -p ./tsconfig.mockr.json && webpack --mode production",
    "build:dev": "tsc -p ./tsconfig.mockr.json && webpack --mode development",
    "clean": "rimraf ./dist",
    "release": "npm test && npm run clean && npm run build",
    "start": "node ./dist/test.js",
    "test": "npm run build && npm run test:lint && npm run test:unit",
    "test:lint": "tslint -p .",
    "test:unit": "jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boschni/server-mockr.git"
  },
  "keywords": [
    "mock",
    "HTTP",
    "server",
    "test",
    "json",
    "API",
    "e2e",
    "verify",
    "stub",
    "fake",
    "dummy"
  ],
  "author": "Niek Bosch <info@niek-bosch.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boschni/server-mockr/issues"
  },
  "homepage": "https://github.com/boschni/server-mockr#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie": "^0.4.0",
    "expect": "^25.1.0",
    "express": "^4.17.1",
    "json-pointer": "^0.6.0",
    "markdown-it": "^10.0.0",
    "node-fetch": "^2.6.0",
    "path-to-regexp": "^6.1.0",
    "strip-ansi": "^6.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cookie": "^0.3.3",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.3",
    "@types/json-pointer": "^1.0.30",
    "@types/markdown-it": "0.0.9",
    "@types/node": "^12.12.28",
    "@types/node-fetch": "^2.5.5",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "css-loader": "^3.4.2",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-no-circular-imports": "^0.7.0",
    "typescript": "^3.8.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  }
}
